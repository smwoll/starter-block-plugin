{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "starter-block-plugin/dynamic-block",
	"version": "1.0",
	"title": "Sky Example Dynamic Block from Starter Block Plugin",
	"category": "starter-block-plugin-examples",
	"icon": "universal-access-alt",
	"attributes": {
		"message": {
			"type": "string",
			"default": "Hello from a dynamic block!"
		}
	},
	"example": {
		"attributes": {
			"message": "Example Dynamic Block"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "starter-block-plugin",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
    "viewScript": "file:./view.js",
	"style": "file:./style-index.css"
}